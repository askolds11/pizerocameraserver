@using picamerasserver.Components.Layout
<MudContainer MaxWidth="MaxWidth.Large">
    <MudPaper MinHeight="@($"calc(100vh - {MyThemeProvider.MyCustomTheme.LayoutProperties.AppbarHeight})")">
        @ChildContent
    </MudPaper>
</MudContainer>

@code {
    [Parameter, EditorRequired]
    public required RenderFragment ChildContent { get; set; }
}